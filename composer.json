{
  "name": "opmvpc/patrons",
  "description": "design patterns examples",
  "keywords": [
    "opmvpc",
    "patrons",
    "design patterns"
  ],
  "homepage": "https://github.com/opmvpc/patrons",
  "license": "MIT",
  "authors": [
    {
      "name": "opmvpc",
      "email": "thibsix@outlook.be",
      "homepage": "https://tsix.be",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4",
    "bartlett/umlwriter": "2.0.0-rc.2"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16",
    "phpunit/phpunit": "^9.3",
    "vimeo/psalm": "^3.11"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/llaville/graphviz"
    }
  ],
  "autoload": {
    "psr-4": {
      "Opmvpc\\Patrons\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Opmvpc\\Patrons\\Tests\\": "tests"
    }
  },
  "scripts": {
    "psalm": "psalm",
    "test": "phpunit",
    "test-coverage": ".\\vendor\\bin\\phpunit --coverage-html coverage",
    "format": ".\\vendor\\bin\\php-cs-fixer fix --allow-risky=yes"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
